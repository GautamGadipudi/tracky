    "version": "0.2.0",
    "configurations": [

    {
            "name": "Test collect",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${cwd}/src/"
            },
            "program": "${cwd}/src/tracky/tests/test_collect.py"
        },
        {
            "name": "Test match",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${cwd}/src/"
            },
            "program": "${cwd}/src/tracky/tests/test_match.py"
        },
        {
            "name": "Program 1: collect good JSON",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${cwd}/src/"
            },
            "program": "${cwd}/src/programs/program1.py",
            "args": [
                "collect",
                "--jsoninputpath", "./data/objects/good.json",
                "--verbose",
                "--outputdirectory",
                "./output/"
            ]
        },
        {
            "name": "Program 1: collect bad JSON",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${cwd}/src/"
            },
            "program": "${cwd}/src/programs/program1.py",
            "args": [
                "collect",
                "--jsoninputpath", "./data/objects/bad.json",
                "--verbose",
                "--outputdirectory",
                "./output/"
            ]
        },
        {
            "name": "Program 1: match <bad> <good>",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${cwd}/src/"
            },
            "program": "${cwd}/src/programs/program1.py",
            "args": [
                "match",
                "--targetfile", "./output/eval/good.jsonl",
                "--jsoninputpath", "./data/objects/bad.json"
            ]
        },
        {
            "name": "Program 1: match <good> <good>",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${cwd}/src/"
            },
            "program": "${cwd}/src/programs/program1.py",
            "args": [
                "match",
                "--targetfile", "./output/eval/good.jsonl",
                "--jsoninputpath", "./data/objects/good.json"
            ]
        },
    ]
}